<query id="insertFileDownloadedLog" action="insert">
    <tables>
        <table name="files_downloaded_log" />
    </tables>
    <columns>
        <column name="file_srl" var="file_srl" notnull="notnull" />
        <column name="upload_target_srl" var="upload_target_srl" notnull="notnull" />     
        <column name="member_srl" var="member_srl" default="0" />        
        <column name="ipaddress" var="ipaddress" default="ipaddress()" />
        <column name="download_count" var="download_count" default="1" />  
        <column name="regdate" var="regdate" default="curdate()" />
    </columns>
</query>
